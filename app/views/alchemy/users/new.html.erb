<div id="login_container">
  <div id="login_box">
    <div id="alchemy_greeting">
      <%= image_tag("alchemy/alchemy-logo.svg") %>
    </div>
    <%= render_message do %>
      <h1><%= _t('Welcome to Alchemy') %></h1>
      <p>
        <%= _t("Please Signup") %>
      </p>
    <% end %>
    <%= alchemy_form_for @user do |f| %>
      <%= render 'alchemy/admin/users/fields', f: f %>
    <% end %>
  </div>
</div>
